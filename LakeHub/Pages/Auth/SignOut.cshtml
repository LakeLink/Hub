@page
@using Microsoft.AspNetCore.Authentication;
@{
    ViewData["Title"] = "Sign Out";
    Layout = "_Layout";
}
@{
    var signOut = HttpContext.SignOutAsync();
    HttpContext.Session.Clear();
    await signOut;
}

